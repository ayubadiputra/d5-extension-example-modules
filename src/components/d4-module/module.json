{
  "name": "example/d4-module",
  "d4Shortcode": "d4_module",
  "title": "Divi 4 Module Hahaha",
  "titles": "Divi 4 Modules",
  "moduleIcon": "example/module-d4",
  "category": "module",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": {
          "adminLabel": {
            "desktop": {
              "value": "D4 Module"
            }
          }
        }
      }
    },
    "title": {
      "type": "object",
      "selector": "{{selector}} .d4_module_title",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h2"
                }
              }
            }
          }
        }
      },
      "inlineEditor": "plainText",
      "elementType": "heading",
      "childrenSanitizer": "et_core_esc_previously",
      "attributes": {
        "class": "d4_module_title"
      }
    },
    "content": {
      "type": "object",
      "selector": "{{selector}} .d4_module_content",
      "attributes": {
        "class": "d4_module_content"
      },
      "tagName": "div",
      "inlineEditor": "richText"
    },
    "myElement": {
      "type": "object",
      "selector": "{{selector}}",
      "attributes": {
        "class": ""
      },
      "tagName": "div",
      "childrenSanitizer": "et_core_esc_previously",
      "settings": {
        "decoration": {
          "bodyFont": {},
          "sizing": {
            "groupType": "group-item",
            "item": {
              "groupSlug": "designSizing",
              "priority": 5,
              "render": true,
              "component": {
                "type": "group",
                "name": "divi/sizing",
                "props": {
                  "grouped": false,
                  "fieldLabel": "My Element",
                  "fields": {
                    "alignment": {
                      "render": false
                    },
                    "height": {
                      "render": false
                    },
                    "maxHeight": {
                      "render": false
                    },
                    "minHeight": {
                      "render": false
                    }
                  }
                }
              }
            }
          }
        }
      },
      "styleProps": {
        "bodyFont": {
          "important": true
        },
        "sizing": {
          "selector": "{{selector}}"
        }
      }
    }
  },
  "customCssFields": {
    "title": {
      "subName": "title",
      "selectorSuffix": " .d4_module_title"
    },
    "content": {
      "subName": "content",
      "selectorSuffix": " .d4_module_content"
    }
  }
}